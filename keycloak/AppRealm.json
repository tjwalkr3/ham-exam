{
  "realm": "AppRealm",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "userManagedAccessAllowed": false,
  "clients": [
    {
      "clientId": "ham-exam",
      "name": "ham-exam",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "frontchannelLogout": false,
      "fullScopeAllowed": true,
      "redirectUris": [
        "https://ham.dragonbytes.org/*",
        "http://localhost:5173/*"
      ],
      "webOrigins": [
        "https://ham.dragonbytes.org",
        "http://localhost:5173"
      ],
      "attributes": {
        "post.logout.redirect.uris": "https://ham.dragonbytes.org/*##http://localhost:5173/*"
      }
    }
  ],
  "users": [
    {
      "username": "test-user",
      "enabled": true,
      "emailVerified": true,
      "email": "test-user@example.com",
      "firstName": "Test",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "test123",
          "temporary": false
        }
      ],
      "requiredActions": [],
      "realmRoles": [
        "default-roles-apprealm"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "default-roles-apprealm",
        "description": "Default roles for AppRealm",
        "composite": true,
        "composites": {
          "realm": [
            "offline_access",
            "uma_authorization"
          ]
        },
        "clientRole": false,
        "containerId": "AppRealm"
      },
      {
        "name": "uma_authorization"
      },
      {
        "name": "offline_access"
      }
    ]
  }
}
